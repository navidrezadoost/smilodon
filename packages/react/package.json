{
  "name": "@native-select/react",
  "version": "0.0.1",
  "description": "React adapter for @native-select/core",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c && tsc --emitDeclarationOnly --outDir dist/types",
    "size": "gzip -c dist/index.js | wc -c"
  },
  "peerDependencies": {
    "@native-select/core": "^0.0.1",
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^18.2.0",
    "rollup": "^4.9.0",
    "typescript": "^5.6.2",
    "react": "^18.2.0"
  },
  "keywords": [
    "react",
    "select",
    "dropdown",
    "virtualization"
  ],
  "license": "MIT",
  "sideEffects": false
}
