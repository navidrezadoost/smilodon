{
  "name": "@smilodon/react",
  "version": "1.0.0",
  "description": "React adapter for @smilodon/core - Production-ready accessible select component",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc --declaration --emitDeclarationOnly --outDir dist/types && vite build && node scripts/cleanup-types.js",
    "size": "gzip -c dist/index.js | wc -c"
  },
  "peerDependencies": {
    "@smilodon/core": "^1.0.0",
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@vitejs/plugin-react": "^4.7.0",
    "react": "^18.2.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.21"
  },
  "keywords": [
    "react",
    "select",
    "dropdown",
    "combobox",
    "autocomplete",
    "searchable",
    "multi-select",
    "virtualization",
    "infinite-scroll",
    "accessible",
    "wcag",
    "aria",
    "typescript",
    "web-components"
  ],
  "license": "MIT",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/smilodon.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/yourusername/smilodon/issues"
  },
  "homepage": "https://github.com/yourusername/smilodon#readme"
}
