{
  "name": "@smilodon/vue",
  "version": "1.0.0",
  "description": "Vue 3 adapter for @smilodon/core - Production-ready accessible select component",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "vue-tsc --declaration --emitDeclarationOnly --outDir dist/types && vite build && node scripts/cleanup-types.js",
    "size": "gzip -c dist/index.js | wc -c"
  },
  "peerDependencies": {
    "@smilodon/core": "^1.0.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.21",
    "vue": "^3.4.0",
    "vue-tsc": "^2.2.12"
  },
  "keywords": [
    "vue",
    "vue3",
    "select",
    "dropdown",
    "combobox",
    "autocomplete",
    "searchable",
    "multi-select",
    "virtualization",
    "infinite-scroll",
    "accessible",
    "wcag",
    "aria",
    "typescript",
    "web-components",
    "composition-api"
  ],
  "license": "MIT",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/smilodon.git",
    "directory": "packages/vue"
  },
  "bugs": {
    "url": "https://github.com/yourusername/smilodon/issues"
  },
  "homepage": "https://github.com/yourusername/smilodon#readme"
}
