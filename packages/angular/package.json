{
  "name": "@smilodon/angular",
  "version": "1.0.4",
  "description": "Production-ready, accessible select component for Angular - part of the Smilodon UI toolkit",
  "type": "module",
  "main": "./dist/fesm2022/smilodon-angular.mjs",
  "module": "./dist/fesm2022/smilodon-angular.mjs",
  "typings": "./dist/types/smilodon-angular.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./dist/types/smilodon-angular.d.ts",
      "default": "./dist/fesm2022/smilodon-angular.mjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "ng-packagr -p ng-package.json",
    "test": "echo \"No tests yet\" && exit 0",
    "size": "gzip -c dist/fesm2022/smilodon-angular.mjs | wc -c",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@angular/common": "^14.0.0 || ^15.0.0 || ^16.0.0 || ^17.0.0 || ^18.0.0 || ^19.0.0 || ^20.0.0 || ^21.0.0",
    "@angular/core": "^14.0.0 || ^15.0.0 || ^16.0.0 || ^17.0.0 || ^18.0.0 || ^19.0.0 || ^20.0.0 || ^21.0.0",
    "@angular/forms": "^14.0.0 || ^15.0.0 || ^16.0.0 || ^17.0.0 || ^18.0.0 || ^19.0.0 || ^20.0.0 || ^21.0.0",
    "@smilodon/core": "^1.0.0"
  },
  "devDependencies": {
    "@angular/common": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "ng-packagr": "^21.0.0",
    "typescript": "~5.9.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.6.0",
    "zone.js": "^0.15.0"
  },
  "keywords": [
    "angular",
    "select",
    "dropdown",
    "combobox",
    "autocomplete",
    "virtualization",
    "infinite-scroll",
    "accessibility",
    "wcag",
    "a11y",
