{
  "name": "@smilodon/vanilla",
  "version": "1.0.0",
  "description": "Vanilla JavaScript helpers for Smilodon Select - part of the Smilodon UI toolkit",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc --emitDeclarationOnly --outDir dist/types && vite build",
    "size": "gzip -c dist/index.mjs | wc -c",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@smilodon/core": "^1.0.0"
  },
  "devDependencies": {
    "typescript": "^5.6.2",
    "vite": "^5.4.11"
  },
  "keywords": [
    "vanilla",
    "javascript",
    "typescript",
    "select",
    "dropdown",
    "combobox",
    "autocomplete",
    "virtualization",
    "infinite-scroll",
    "accessibility",
    "wcag",
    "a11y",
    "smilodon",
    "web-component"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navidrezadoost/smilodon.git",
    "directory": "packages/vanilla"
  },
  "bugs": {
    "url": "https://github.com/navidrezadoost/smilodon/issues"
  },
  "homepage": "https://github.com/navidrezadoost/smilodon#readme",
  "author": "Navid Rezadoost <navidrezadoost07@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
